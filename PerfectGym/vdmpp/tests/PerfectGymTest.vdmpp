class PerfectGymTest is subclass of MyTestCase

instance variables

  owner1: Owner := new Owner("Rui");
	owner2: Owner := new Owner("Tiago");

	club1: Club := new Club("Bombados", owner1, 25);
	club2: Club := new Club("PuxaFerro", owner2, 30);	

	perfectGym1: PerfectGym := new PerfectGym();	

	
operations
	public Run: () ==> ()
	Run() == (
		IO`println("\nPerfectGym Tests");

		perfectGym1.addClub(club1);
		perfectGym1.addClub(club2);
			
		assertEqual({"Bombados"|->club1, "PuxaFerro"|->club2}, perfectGym1.getClubs());
			
		IO`println("\nFinalizing PerfectGym Tests");
	);
	
end PerfectGymTest