class TrainerTest is subclass of MyTestCase

instance variables

	trainer1: Trainer := new Trainer("Vasco", 25, <Male>, "brazilian");

	client1: Client := new Client("Maria", 23, <Female>, "portuguese");
	client2: Client := new Client("Jorge", 25, <Male>, "spanish");

  	trainingSession1: TrainingSession := new TrainingSession("Aula iniciante", trainer1, client1, <Monday>, 
		Utils`CreateHour(14, 00), 
	  	Utils`CreateHour(15, 00), 
	  	Utils`CreateDate(2019, 01, 14));

	gymClass1: GymClass := new GymClass("Aula de baixa intensidade", "Pilates", trainer1, <Tuesday>, 
		Utils`CreateHour(09, 00), 
		Utils`CreateHour(10, 00), 
		Utils`CreateDate(2019, 01, 15));
	
	
operations
	public Run: () ==> ()
	Run() == (
		IO`println("\nTrainer Tests");

		trainer1.addTrainee(client1);
		trainer1.addTrainee(client2);
		assertEqual({client1, client2}, trainer1.getTrainees());
		trainer1.removeTrainee(client2);
		assertEqual({client1}, trainer1.getTrainees());

		trainer1.addGymClass(gymClass1);
		assertEqual({gymClass1}, trainer1.getClasses());

		trainer1.addTrainingSession(trainingSession1);
		assertEqual({trainingSession1}, trainer1.getTrainingSessions());

		trainer1.getActivity(true);

		IO`println("Finalizing Trainer Tests");

	);
	
end TrainerTest